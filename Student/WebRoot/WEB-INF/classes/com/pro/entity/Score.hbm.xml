<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.pro.entity.Score" table="t_score">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="native"/>
        </id>
        <property access="field" column="t_name" length="200" name="name"/>
        <property access="field" column="t_score" length="200" name="score"/>
        <property access="field" column="t_stuno" length="200" name="stuno"/>
        <property access="field" column="t_clazzname" length="200" name="clazzname"/>
        <property access="field" column="t_year" length="200" name="year"/>
        <property access="field" column="t_type" length="200" name="type"/>
        <property access="field" column="t_ismdy" length="200" name="ismdy"/>
        <property access="field" column="t_indate" length="200" name="indate"/>
        <property access="field" column="t_lastdate" length="200" name="lastdate"/>
        <property access="field" column="t_hasapply" length="200" name="hasapply"/>
        <property access="field" column="t_school" length="200" name="school"/>
        <property access="field" column="t_number" length="11" name="number"/>
        <property access="field" column="t_status" length="11" name="status"/>
        <property access="field" column="t_mandatory" length="11" name="mandatory"/>
    </class>
</hibernate-mapping>
