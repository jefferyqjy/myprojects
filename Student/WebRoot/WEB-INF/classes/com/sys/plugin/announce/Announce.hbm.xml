<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sys.plugin.announce.Announce" table="t_announce">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="native"/>
        </id>
        <property access="field" column="t_title" length="100" name="title"/>
        <property access="field" column="t_content" length="2000" name="content"/>
        <property access="field" column="t_createTime" length="40" name="createTime"/>
        <property access="field" column="t_lastModifyTime" length="40" name="lastModifyTime"/>
        <property access="field" column="t_createdUser" length="80" name="createdUser"/>
        <property access="field" column="t_modifiedUser" length="80" name="modifyBy"/>
        <property access="field" column="t_level" length="4" name="level"/>
    </class>
</hibernate-mapping>
