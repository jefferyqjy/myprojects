<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.pro.entity.Course" table="t_course">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="native"/>
        </id>
        <property access="field" column="t_name" length="200" name="name"/>
        <property access="field" column="t_number" length="11" name="number"/>
        <property access="field" column="t_pass" length="11" name="pass"/>
        <property access="field" column="t_mandatory" length="11" name="mandatory"/>
        <property access="field" column="t_content" length="2000" name="content"/>
    </class>
</hibernate-mapping>
