<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.pro.entity.CourseStudent" table="t_course_student">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="native"/>
        </id>
        <property access="field" column="t_name" length="50" name="name"/>
        <property access="field" column="t_stuno" length="200" name="stuno"/>
        <property access="field" column="t_year" length="11" name="year"/>
        <property access="field" column="t_type" length="11" name="type"/>
	    </class>
</hibernate-mapping>
